/**index.wxss**/



.swiper-item {
  width: 100%;
  height: 100%;
}

.nav_menu {
  padding: 10px 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.nav_menu .list {
  display: flex;
  font-size: 12px;
  color: #848484;
}

.nav_menu .list image {
  width: 128rpx;
  height: 140rpx;
}

.main > navigator  image {
  width: 100%;
  height: 80rpx;
}

.main .floor {
  display: flex;
  justify-content: space-between;
}

.floor {
  padding: 20rpx 20rpx;
}
.floor .left image {
  width: 230rpx;
  height: 370rpx;
   border-radius: 5rpx;
}

.floor .right {
  display: flex;
  flex-wrap: wrap;
  justify-content: start;
}

.floor .right image {
  width: 230rpx;
  height: 180rpx;
  border-radius: 5rpx;
  margin-left:10rpx;
}
